    <sql id="smsProducerVo">
          SELECT
	p.producer_id,
	p.NAME,
	p.CODE,
	p.address,
	p.type,
	p.platform,
	p.STATUS,
	p.pay_type,
	p.max_money,
	p.bank,
	p.bank_address,
	p.bank_user,
	p.account,
	p.before_pay,
	p.credit_day,
