	p.is_free_tax,
	p.total_price,
	p.delivery_day,
	p.quality,
	p.main_product,
	p.contacts,
	p.mobile,
	p.phone,
	p.wangwang,
	p.qq,
	p.spare_contacts,
	p.spare_mobile,
	p.spare_phone,
	p.spare_qq,
	p.sort,
	p.certificate,
	p.remark,
	p.ext_json,
